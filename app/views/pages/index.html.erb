<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="icon" href="../../favicon.ico">

    <title>Jumbotron Template for Bootstrap</title>

    <link href="bootstrap.min.css" rel="stylesheet">

    <link href="jumbotron.css" rel="stylesheet">

    <script src="../../assets/js/ie-emulation-modes-warning.js"></script>


  </head>

  <body>

    <!-- fixed top navigation bar -->
    <div class="navbar navbar-inverse navbar-fixed-top" role="navigation">
      <div class="container">
        <div class="navbar-header">
          <a class="navbar-brand" href="#">gCamp</a>
        </div>
      </div>
    </div>

    <!-- main page container -->
    <div class="row">
      <div class="col-md-10 col-md-offset-1 content">

        <!-- Jumbrotron row -->
        <div class="row">
          <div class="col-md-12">

            <div class="container tron-top-buffer">
              <div class="jumbotron box" >
                <h1>gCamp</h1>
                <p>Your life, organized</p>
              </div>
            </div>

          </div>
        </div>
        <!-- end of jumbotron -->

        <!-- Panel contrainer -->
        <div class="container">
          <div class="row">

            <div class="col-md-4">

              <div class="panel panel-default panel-image">
                <div class="panel-body">
                  <img class="panelImage" src="https://farm3.staticflickr.com/2833/8865586006_c5ce2a79e6_n.jpg" />
                </div>
              </div>

              <div class="panel panel-info panel-content">
                <div class="panel-heading">
                  <h4 class="panel-title">Tasks</h4>
                </div>
                <div class="panel-body">
                  <p>Grouped by projects and lists. Just the way you like 'em</p>
                </div>
              </div>
            </div>

            <div class="col-md-4">

              <div class="panel panel-default panel-image">
                <div class="panel-body">
                  <img class="panelImage" src="https://farm4.staticflickr.com/3832/8865584540_b5307795be_n.jpg" />
                </div>
              </div>

              <div class="panel panel-info panel-content">
                <div class="panel-heading">
                  <h4 class="panel-title">Comments</h4>
                </div>
                <div class="panel-body">
                  <ul>
                    <li>Upload</li>
                    <li>Comment</li>
                    <li>Revise</li>
                  </ul>
                </div>
              </div>
            </div>

            <div class="col-md-4">

              <div class="panel panel-default panel-image">
                <div class="panel-body">
                  <img class="panelImage" src="https://farm4.staticflickr.com/3821/9232183203_859067da50_n.jpg" />
                </div>
              </div>

              <div class="panel panel-info panel-content">
                <div class="panel-heading">
                  <h4 class="panel-title">Comments</h4>
                </div>
                <div class="panel-body">
                  <ul>
                    <li>Comment on tasks and documents</li>
                    <li>Get email notifications</li>
                  </ul>
                </div>
              </div>
            </div>

          </div>
        </div>
        <!-- End of panel container -->
      </div>
    </div>
    <!--End of main page container-->


    <!-- Bootstrap core JavaScript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
    <script src="../../dist/js/bootstrap.min.js"></script>
    <!-- IE10 viewport hack for Surface/desktop Windows 8 bug -->
    <script src="../../assets/js/ie10-viewport-bug-workaround.js"></script>
  </body>
</html>
